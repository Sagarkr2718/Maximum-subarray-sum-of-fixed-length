public class Solution {
    public long solve(int[] A, int B) {
       int n = A.length;
       int ans = 0;
       for(int i = 0 ; i<B ; i++){
           ans+=A[i];
       }
       int sum = ans;
       for(int i=1 ; B<n ; i++,B++){
           
           sum=sum-A[i-1]+A[B];
           if(sum>ans){
               ans = sum;
           }
       }
       return ans;
    }
}
